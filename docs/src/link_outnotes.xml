<?xml version="1.0" encoding="UTF-8"?>
<configentry>
    <keyword>OUTNOTES</keyword>

    <scope>LINK</scope>

    <anchor>LINK_OUTNOTES</anchor>

    <definition>OUTNOTES

        <meta>url</meta>
    </definition>

    <definition>OUTNOTES

        <meta>notes text</meta>
    </definition>

    <description xmlns="http://www.w3.org/1999/xhtml">
        <p>Same as NOTES, but specifies a pop-up text box for only the 'out' side of a
        link.</p>
    </description>

    <changes>
        <change version="0.95">Added IN/OUT support
        </change>
    </changes>
</configentry>
